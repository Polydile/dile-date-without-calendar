{
  "name": "dile-date-without-calendar",
  "version": "1.0.3",
  "description": "Web component that selects a date without a calendar, for an input text field, based on LitElement.",
  "scripts": {
    "start": "es-dev-server --node-resolve --watch --open",
    "start:compatibility": "es-dev-server --compatibility all --node-resolve --watch --open",
    "site:build": "npm run storybook:build",
    "storybook": "start-storybook -p 9001",
    "storybook:build": "build-storybook git-o _site -s storybook-static",
    "start:build": "cd dist && es-dev-server --open",
    "build-develo": "webpack --mode development --output ./dist/dile-date-without-calendar.js",
    "build": "webpack --mode production --output ./dist/dile-date-without-calendar.min.js"
  },
  "keywords": [
    "input",
    "webcomponents",
    "litelement",
    "text",
    "field",
    "date"
  ],
  "author": "Dennys Jose Marquez Reyes <dennysjmarquez@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "lit-element": "^2.2.0",
    "lit-html": "^1.1.1",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@open-wc/demoing-storybook": "^0.2.0",
    "es-dev-server": "^1.7.2",
    "@open-wc/building-webpack": "^2.1.0",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.3.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Polydile/dile-date-without-calendar.git"
  },
  "homepage": "https://github.com/Polydile/dile-date-without-calendar#readme"
}
